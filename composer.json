{
  "name": "smartemailing/types",
  "description": "Lightweight collection of handy PHP value objects",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "strict types",
    "value objects"
  ],
  "authors": [
    {
      "name": "Martin Strouhal",
      "email": "martin@smartemailing.cz"
    }
  ],
  "require": {
    "php": "^8.1",
    "nette/utils": "^3.0",
    "nette/http": "^3.0",
    "egulias/email-validator": "^3.1.1"
  },
  "require-dev": {
    "jschaedl/iban-validation": "^2.0",
    "nepada/phpstan-nette-tester": "^1.0",
    "nette/tester": "^2.4",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-nette": "^1.1",
    "slevomat/coding-standard": "^8.5",
    "tracy/tracy": "^2.8"
  },
  "suggest": {
    "jschaedl/iban-validation": "Allows using SmartEmailing\\Types\\Iban type."
  },
  "conflict": {
    "symfony/options-resolver": "<5.4"
  },
  "autoload": {
    "psr-4": {
      "SmartEmailing\\Types\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmartEmailing\\Types\\": [
        "tests"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
